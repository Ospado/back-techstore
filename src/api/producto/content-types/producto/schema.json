{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Nombre",
      "required": true
    },
    "Descripcion": {
      "type": "text",
      "required": true
    },
    "Precio": {
      "type": "decimal",
      "required": true
    },
    "oldPrice": {
      "type": "decimal"
    },
    "Imagen": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Stock": {
      "type": "integer",
      "required": true
    },
    "features": {
      "type": "text"
    },
    "isNew": {
      "type": "boolean"
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "productos"
    }
  }
}
