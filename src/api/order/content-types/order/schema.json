{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "OrderNumber": {
      "type": "string",
      "required": true
    },
    "Customer": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "Date": {
      "type": "datetime",
      "required": true
    },
    "StatusOrder": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Pendiente",
        "Procesado",
        "Completo",
        "Cancelado"
      ]
    },
    "Total": {
      "type": "decimal",
      "required": true
    },
    "Direccion": {
      "type": "text",
      "required": true
    },
    "MetodoDePago": {
      "type": "string",
      "required": true
    },
    "Items": {
      "type": "component",
      "component": "product.order-item",
      "repeatable": true
    }
  }
}
